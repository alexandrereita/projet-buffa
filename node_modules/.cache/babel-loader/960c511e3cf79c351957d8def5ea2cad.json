,IAAA,GAAA,KAAA;AACA;AANA,GAzBA;AAiCA,EAAA,OAjCA,qBAiCA;AACA,IAAA,OAAA,CAAA,GAAA,CAAA,aAAA;AACA,SAAA,wBAAA;AACA,GApCA;AAqCA,EAAA,OAAA,EAAA,mBAAA;AACA,IAAA,OAAA,CAAA,GAAA,CAAA,MAAA;AACA,GAvCA;AAwCA,EAAA,OAAA,EAAA;AACA,IAAA,wBADA,sCACA;AAAA;;AACA,UAAA,GAAA,GAAA,gDAAA,KAAA,IAAA,GAAA,YAAA,GAAA,KAAA,OAAA,GAAA,QAAA,GAAA,KAAA,YAAA;AAEA,MAAA,KAAA,CAAA,GAAA,CAAA,CACA,IADA,CACA,UAAA,YAAA,EAAA;AACA,eAAA,YAAA,CAAA,IAAA,GACA,IADA,CACA,UAAA,GAAA,EAAA;AACA;AACA,UAAA,KAAA,CAAA,WAAA,GAAA,GAAA,CAAA,IAAA;AACA,UAAA,KAAA,CAAA,KAAA,GAAA,GAAA,CAAA,KAAA;AACA,UAAA,KAAA,CAAA,cAAA,GAAA,IAAA,CAAA,KAAA,CAAA,KAAA,CAAA,KAAA,GAAA,KAAA,CAAA,OAAA,CAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,WAAA;AACA,SAPA,CAAA;AAQA,OAVA,EAWA,KAXA,CAWA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA,CAAA,GAAA;AACA,OAbA;AAcA,KAlBA;AAmBA,IAAA,cAnBA,4BAmBA;AACA,WAAA,IAAA;AACA,WAAA,wBAAA;AACA,KAtBA;AAuBA,IAAA,YAvBA,0BAuBA;AACA,WAAA,IAAA;AACA,WAAA,wBAAA;AACA,KA1BA;AA2BA,IAAA,iBA3BA,6BA2BA,KA3BA,EA2BA;AACA;AACA,MAAA,KAAA,CAAA,cAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,IAAA;AACA,WAAA,WAAA,CAAA,IAAA,CACA;AACA,QAAA,IAAA,EAAA,KAAA,IADA;AAEA,QAAA,OAAA,EAAA,KAAA;AAFA,OADA;AAMA,WAAA,IAAA,GAAA,EAAA;AACA,WAAA,OAAA,GAAA,EAAA;AACA;AAvCA;AAxCA,CAAA","sourcesContent":["<template>\n  <div id=\"app\">\n    <div class=\"form\" v-if=\"show\">\n    <form v-on:submit=\"ajouterRestaurant\">\n        <label>\n            Nom : <input type=\"text\" required v-model=\"name\">\n        </label>\n        <label>\n            Cuisine : <input type=\"text\" required v-model=\"cuisine\">\n        </label>\n\n        <button>Ajouter</button>\n    </form>\n    <div class=\"research\">\n        Recherche par nom : \n        <input type=\"text\" v-model=\"nomRecherche\"\n        v-on:input=\"getRestaurantsFromServer()\"\n        >\n    </div>\n    \n    <div class=\"slidecontainer\">\n        <input type=\"range\" min=\"5\" max=\"100\" v-model=\"nbResto\" value=\"50\" class=\"slider\" id=\"myRange\"\n            v-on:change=\"getRestaurantsFromServer()\"\n        >\n        <div>Nb resto par page : {{nbResto}}</div>\n    </div>\n    <button v-on:click=\"pagePrecedente()\" :disabled=\"page === 0\">Précédent</button>\n    <button v-on:click=\"pageSuivante()\" :disabled=\"page === nbPageResultat\">Suivant</button>\n    </div>\n    <RestaurantsListe :liste=\"this.restaurants\" v-if=\"show\"/>\n    <router-view></router-view>\n  </div>\n</template>\n\n<script>\nimport RestaurantsListe from './components/RestaurantsListe.vue'\n\nexport default {\n  name: 'app',\n  components: {\n    RestaurantsListe\n  },\n   data:() => {return {\n     show: true,\n            page: 0,\n            count: 0,\n            nbResto:50,\n            restaurants: [\n                {\n                    name: 'café de Paris',\n                    cuisine: 'Française'\n                },\n                {\n                    name: 'Sun City Café',\n                    cuisine: 'Américaine'\n                }\n            ],\n            nomRecherche: '',\n            nbPageResultat: 0,\n            name: '',\n            cuisine: '',\n        }},\n  watch: {\n      $route (to){\n          console.log('watch route', to)\n          console.log('if path exact', to.path === '/')\n          if(to.path === '/') return this.show = true\n          else return this.show = false\n      }\n  },\n  mounted(){\n    console.log(\"BEFORE HTML\");\n    this.getRestaurantsFromServer();\n  },\n  created: () => {\n    console.log('heyy')\n  },\n  methods: {\n            getRestaurantsFromServer() {\n                let url = \"http://localhost:8080/api/restaurants?page=\" + this.page + \"&pagesize=\"+ this.nbResto + \"&name=\" + this.nomRecherche;\n\n                fetch(url)\n                    .then(responseJSON => {\n                        return responseJSON.json()\n                        .then(res => {\n                            // Maintenant res est un vrai objet JavaScript\n                            this.restaurants = res.data;\n                            this.count = res.count;\n                            this.nbPageResultat = Math.floor(this.count / this.nbResto);\n                            console.log(this.restaurants);\n                        });\n                    })\n                    .catch(function (err) {\n                        console.log(err.msg);\n                });\n            },\n            pagePrecedente() {\n                this.page--;\n                this.getRestaurantsFromServer();\n            },\n            pageSuivante() {\n                this.page++;\n                this.getRestaurantsFromServer();\n            },\n            ajouterRestaurant(event) {\n                // eviter le comportement par defaut\n                event.preventDefault();\n                console.log(this.name);\n                this.restaurants.push(\n                    {\n                        name: this.name,\n                        cuisine: this.cuisine\n                    }\n                );\n                this.name = \"\";\n                this.cuisine = \"\";\n            }\n  }\n}\n</script>\n\n<style>\n#app {\n  font-family: 'Avenir', Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n}\n</style>\n"],"sourceRoot":"src"}]}